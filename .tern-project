{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "underscore",
    "jquery",
    "chai"
  ],
  "loadEagerly": [
    "app/**/*.js",
    "app/util/**/*.js",
    "app/util/ghobot/**/*.js",
    "app/util/ghobot/engine/**/*.js"
  ],
  "dontLoad": [
    "node_modules/**",
    "path/to/your/js/**/*.js"
  ],
  "plugins": {
    "modules": {},
    "es_modules": {},
    "node": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    },
    "requirejs": {
      "baseURL": "",
      "paths": "",
      "override": ""
    }
  }
}